<div id="heart"><span class="oi oi-check text-success"></span></div>

{{#with data}}
<div id="go-page">

	<div class="container">

		<div class="progress-container">
			<ol class="progressBar">
				<li class="progress-bar-step is-complete">
				<svg class="progress-bar-icon"><use xlink:href="#checkmark-bold"/></svg>
				<span class="progress-bar-stepLabel">
			    	{{#unless setting}}
			    		Choose Setting
			    	{{/unless}}
			    	{{#if setting}}
			    		Selected {{setting}}
			    	{{/if}}
				</span>
				</li>
				<li class="progress-bar-step is-complete">
				<svg class="progress-bar-icon"><use xlink:href="#checkmark-bold"/></svg>
				<span class="progress-bar-stepLabel">Customize Stretches</span>
				</li>
				<li class="progress-bar-step is-complete">
				<svg class="progress-bar-icon"><use xlink:href="#checkmark-bold"/></svg>
				<span class="progress-bar-stepLabel">Ready to Go!</span>
				</li>
			</ol>
		</div> <!-- progress-container -->

		<svg xmlns="http://www.w3.org/2000/svg" width="0" height="0">
			<symbol id="checkmark-bold" viewBox="0 0 24 24">
			<path d="M20.285 2l-11.285 11.567-5.286-5.011-3.714 3.716 9 8.728 15-15.285z"/>
			</symbol>
		</svg>

		{{#if loggedIn}}
		<div class="row">
			<div class="col text-right">
				<!-- Fav form -->
				<form id="fav-form">

					<input class="fav-name" type="hidden" name="name" value="">
					<input class="fav-url" type="hidden" name="url" value="">

					<!-- Fav form submit button -->
						<span id="fav-form-submit" class="oi oi-bookmark"></span>				
				</form> <!-- #fav-form -->
			</div>
		</div>
		{{/if}}

		<div class="row">
			<div class="col text-center">
				<h2 id="routine-name">Go</h2>
			</div>
		</div>

		<div class="row">
			<div class="col">
				
					<p id="instructions">Press <span style="padding: 0 0.3rem" class="oi oi-media-play"></span> when you are ready to start working and receiving alerts.</p>
				
			</div>
		</div>
		<div class="row no-utters">
			<div class="col text-center pause-panel">
				<div class="panel d-flex align-items-center justify-content-center">
					<span id="pause-button" class="oi oi-media-pause"></span>
				</div>
			</div>
			<div class="col text-center">
				<div class="panel d-flex align-items-center justify-content-center">
					<span id="start-button" class="oi oi-media-play"></span>
					<span id="stop-button" class="oi oi-media-stop"></span>
				</div>
			</div>
		</div>

		<div class="row" style="margin-top: 1.5rem">
			<div class="col text-center">
				<div class="panel">
					<div class="curr-stretch-img">
						<img src="https://s21.q4cdn.com/861911615/files/images/placeholder.png">
					</div>
				</div>

			</div>
		</div>


		<div class="pagination d-flex justify-content-between">

			<button class="btn btn-primary back-button" type="button">
				Back
			</button> 


		</div>  <!-- .pagination -->

	</div> <!-- .container -->
	
</div> <!-- #go-page -->
{{/with}}

<!-- go.js -->
<script src="js/go.js"></script>


